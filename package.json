{
  "name": "aperoland-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node ace serve --watch",
    "build": "node ace build --production",
    "start": "node server.js",
    "test": "node ace test",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptApp",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.9.6",
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@hotwired/stimulus": "^3.2.2",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/runner": "2.5.1",
    "@symfony/stimulus-bridge": "^3.2.2",
    "@symfony/webpack-encore": "4.1.1",
    "@types/proxy-addr": "^2.0.3",
    "@types/source-map-support": "^0.5.10",
    "adonis-preset-ts": "^2.1.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.1",
    "jquery": "^3.7.1",
    "pino-pretty": "^10.2.3",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.6",
    "typescript": "~4.6",
    "webpack": "^5.72",
    "webpack-cli": "^4.9.1",
    "youch": "^3.3.3",
    "youch-terminal": "^2.2.3"
  },
  "dependencies": {
    "@adonisjs/core": "^5.8.0",
    "@adonisjs/lucid": "^18.4.2",
    "@adonisjs/repl": "^3.1.0",
    "@adonisjs/session": "^6.2.0",
    "@adonisjs/shield": "^7.0.0",
    "@adonisjs/view": "^6.1.0",
    "luxon": "^3.4.4",
    "mysql2": "^3.6.5",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21"
  }
}
